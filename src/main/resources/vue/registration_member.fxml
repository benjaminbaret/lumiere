<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vue.RegistrationMember">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="1000.0" style="-fx-background-color: fff;">
         <left>
            <Pane prefHeight="600.0" prefWidth="229.0" style="-fx-background-color: #506FFF;" BorderPane.alignment="CENTER">
               <children>
                  <Button layoutX="76.0" layoutY="37.0" mnemonicParsing="false" onAction="#quitButtonClicked" style="-fx-background-color: transparent;">
                     <graphic>
                        <ImageView fitHeight="75.0" fitWidth="61.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../img/cross-button.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
            </Pane>
         </left>
         <center>
            <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Pane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <PasswordField fx:id="passwordField" layoutX="450.0" layoutY="329.0" prefHeight="30.0" prefWidth="242.0" promptText="Password">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </PasswordField>
                        <Text layoutX="26.0" layoutY="77.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add member">
                           <font>
                              <Font name="System Bold" size="40.0" />
                           </font>
                        </Text>
                        <Line endX="570.0" fill="BLACK" layoutX="127.0" layoutY="87.0" startX="-100.0" stroke="#c1c1c1" strokeWidth="2.0" />
                        <TextField fx:id="firstNameField" layoutX="26.0" layoutY="189.0" prefHeight="30.0" prefWidth="242.0" promptText="First name">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </TextField>
                        <TextField fx:id="lastNameField" layoutX="450.0" layoutY="189.0" prefHeight="30.0" prefWidth="242.0" promptText="Last name">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </TextField>
                        <TextField fx:id="emailField" layoutX="25.0" layoutY="259.0" prefHeight="30.0" prefWidth="242.0" promptText="e-mail address">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </TextField>
                        <TextField fx:id="phoneNumberField" layoutX="25.0" layoutY="329.0" prefHeight="30.0" prefWidth="242.0" promptText="phone number">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </TextField>
                        <DatePicker fx:id="dateOfBirthField" editable="false" layoutX="451.0" layoutY="262.0" prefHeight="30.0" prefWidth="242.0" />
                        <Button layoutX="25.0" layoutY="430.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="123.0" style="-fx-background-color: c1c1c1;" text="Reset All" textFill="WHITE">
                           <font>
                              <Font size="16.0" />
                           </font></Button>
                        <Button layoutX="569.0" layoutY="430.0" mnemonicParsing="false" onAction="#handleContinueButtonAction" prefHeight="25.0" prefWidth="123.0" style="-fx-background-color: #506FFF;" text="Continue" textFill="WHITE">
                           <font>
                              <Font size="16.0" />
                           </font></Button>
                        <DialogPane fx:id="errorDialogPane" layoutX="205.0" layoutY="273.0" style="-fx-border-color: black;" visible="false">
                           <content>
                              <Pane prefHeight="100.0" prefWidth="400.0">
                                 <children>
                                    <Button fx:id="okDialogButton" layoutX="318.0" layoutY="61.0" mnemonicParsing="false" onAction="#okButtonClicked" text="OK" />
                                    <Text layoutX="121.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Entrees are not correct.&#10;email = email@@example.com&#10;phone number = 0879765767" />
                                 </children>
                              </Pane>
                           </content></DialogPane>
                     </children>
                  </Pane>
               </children>
            </StackPane>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
